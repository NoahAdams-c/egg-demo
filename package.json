{
  "name": "eggjs-demo",
  "version": "1.0.0",
  "description": "egg.js demo",
  "main": "index.js",
  "scripts": {
    "pm2:start": "pm2 start -n egg-demo npm -- run start",
    "start": "egg-bin dev --sticky",
    "local": "EGG_SERVER_ENV=local npm -- run start",
    "prod": "EGG_SERVER_ENV=prod npm -- run start",
    "db-generate": "npx sequelize migration:generate",
    "db-migrate": "npx sequelize db:migrate",
    "db-undo-last": "npx sequelize db:migrate:undo",
    "db-undo-all": "npx sequelize db:migrate:undo:all",
    "db-seed": "npx sequelize-cli db:seed:all",
    "db-seed-undo": "npx sequelize-cli db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "chenchen",
  "license": "ISC",
  "dependencies": {
    "cc-vue-util": "^1.0.8",
    "egg": "^2.23.0",
    "egg-bin": "^4.13.0",
    "egg-cors": "^2.2.2",
    "egg-mysql": "^3.0.0",
    "egg-redis": "^2.4.0",
    "egg-sequelize": "^5.2.0",
    "egg-socket.io": "^4.1.6",
    "egg-validate": "^2.0.2",
    "mockjs": "^1.0.1-beta3",
    "mysql2": "^1.7.0",
    "sequelize-cli": "^5.5.0"
  }
}
